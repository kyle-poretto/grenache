<h1>
  Welcome, <%= current_user.full_name %>!
</h1>

<div>
  <%= image_tag current_user.picture_url %>
</div>

<table>
  <thead>
    <tr>
      <th>content</th>
      <th>published at</th>
    </tr>
  </thead>
  <tbody>
    <% @posts.each do |post| %>
      <tr>
        <td><%= post.content %></td>
        <td><%= post.created_at %></td>
      </tr>
    <% end %>
  </tbody>
</table>
